<h1>Seeder, our Data Center</h1>

<div class="row">
  <div class="col-md-4">{{number-box title="Posts" number=posts.length}}</div>
  <div class="col-md-4">{{number-box title="Comments" number=comments.length}}</div>
</div>

{{seeder-block
    sectionTitle='Posts'
    counter=postsCounter
    generateAction='generatePosts'
    deleteAction='deletePosts'
    createReady=libDone
    deleteReady=libDelDone
}}
